{
  "name": "cdk-template-typescript",
  "version": "1.0.0",
  "description": "A CDK Node.js TypeScript template",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "tsc && esbuild src/index.ts --bundle --platform=node --outdir=dist",
    "lint": "eslint . --color",
    "prettier": "prettier --check .",
    "test": "jest",
    "test:watch": "jest --watch",
    "watch": "tsc -w",
    "prepare": "husky",
    "cdk": "cdk"
  },
  "devDependencies": {
    "@eslint/compat": "1.2.9",
    "@eslint/js": "9.27.0",
    "@types/jest": "29.5.14",
    "@types/node": "22.15.29",
    "aws-cdk": "2.1017.1",
    "esbuild": "0.25.5",
    "eslint": "9.27.0",
    "eslint-config-prettier": "10.1.5",
    "husky": "9.1.7",
    "jest": "29.7.0",
    "lint-staged": "16.0.0",
    "prettier": "3.5.3",
    "ts-jest": "29.3.4",
    "ts-node": "10.9.2",
    "typescript": "5.8.3",
    "typescript-eslint": "8.32.1"
  },
  "dependencies": {
    "aws-cdk-lib": "2.198.0",
    "constructs": "10.4.2"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown .",
    "*.ts": "eslint --color --fix --max-warnings=0"
  },
  "volta": {
    "node": "22.16.0",
    "npm": "11.4.1"
  }
}
